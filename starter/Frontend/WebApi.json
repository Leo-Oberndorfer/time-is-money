{
  "openapi": "3.0.1",
  "info": {
    "title": "WebApi | v1",
    "version": "1.0.0"
  },
  "paths": {
    "/ping": {
      "get": {
        "tags": [
          "WebApi"
        ],
        "description": "A simple ping endpoint to check if the service is running.",
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "text/plain": {
                "schema": {
                  "type": "string"
                }
              }
            }
          }
        }
      }
    },
    "/commutes/import": {
      "post": {
        "tags": [
          "Commutes"
        ],
        "requestBody": {
          "content": {
            "multipart/form-data": {
              "schema": {
                "required": [
                  "file"
                ],
                "type": "object",
                "properties": {
                  "file": {
                    "type": "string",
                    "format": "binary"
                  }
                }
              }
            }
          },
          "required": true
        },
        "responses": {
          "201": {
            "description": "Created",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CommuteDetailsDto"
                }
              }
            }
          },
          "400": {
            "description": "Bad Request",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CommuteUploadErrorDto"
                }
              }
            }
          }
        }
      }
    },
    "/commutes": {
      "get": {
        "tags": [
          "Commutes"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "type": "array",
                  "items": {
                    "$ref": "#/components/schemas/CommuteListItemDto"
                  }
                }
              }
            }
          }
        }
      }
    },
    "/commutes/{id}": {
      "get": {
        "tags": [
          "Commutes"
        ],
        "parameters": [
          {
            "name": "id",
            "in": "path",
            "required": true,
            "schema": {
              "type": "integer",
              "format": "int32"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CommuteDetailsDto"
                }
              }
            }
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    },
    "/commutes/statistics": {
      "get": {
        "tags": [
          "Commutes"
        ],
        "responses": {
          "200": {
            "description": "OK",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/CommuteStatisticsDto"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "CarCommuteDto": {
        "required": [
          "distanceKm",
          "durationMinutes",
          "averageConsumptionLPer100Km",
          "spentEur",
          "additionalPassengers"
        ],
        "type": "object",
        "properties": {
          "distanceKm": {
            "type": "number",
            "format": "double"
          },
          "durationMinutes": {
            "type": "integer",
            "format": "int32"
          },
          "averageConsumptionLPer100Km": {
            "type": "number",
            "format": "double"
          },
          "spentEur": {
            "type": "number",
            "format": "double"
          },
          "additionalPassengers": {
            "type": "integer",
            "format": "int32",
            "nullable": true
          }
        }
      },
      "CommuteAnalysisDto": {
        "required": [
          "carArrivalUtc",
          "publicArrivalUtc",
          "carPoints",
          "publicPoints",
          "decisionVerdict",
          "eurPerMinutePerPerson"
        ],
        "type": "object",
        "properties": {
          "carArrivalUtc": {
            "type": "string",
            "format": "date-time"
          },
          "publicArrivalUtc": {
            "type": "string",
            "format": "date-time"
          },
          "carPoints": {
            "type": "integer",
            "format": "int32"
          },
          "publicPoints": {
            "type": "integer",
            "format": "int32"
          },
          "decisionVerdict": {
            "type": "string"
          },
          "eurPerMinutePerPerson": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        },
        "nullable": true
      },
      "CommuteDetailsDto": {
        "required": [
          "id",
          "departureUtc",
          "destination",
          "chosenTravelMethod",
          "scheduledArrivalUtc",
          "car",
          "public",
          "analysis"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "departureUtc": {
            "type": "string",
            "format": "date-time"
          },
          "destination": {
            "type": "string"
          },
          "chosenTravelMethod": {
            "type": "string"
          },
          "scheduledArrivalUtc": {
            "type": "string",
            "format": "date-time",
            "nullable": true
          },
          "car": {
            "$ref": "#/components/schemas/CarCommuteDto"
          },
          "public": {
            "$ref": "#/components/schemas/PublicCommuteDto"
          },
          "analysis": {
            "$ref": "#/components/schemas/CommuteAnalysisDto"
          }
        }
      },
      "CommuteListItemDto": {
        "required": [
          "id",
          "departureUtc",
          "destination",
          "chosenTravelMethod",
          "carDurationMinutes",
          "publicDurationMinutes",
          "decisionVerdict",
          "eurPerMinutePerPerson"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "departureUtc": {
            "type": "string",
            "format": "date-time"
          },
          "destination": {
            "type": "string"
          },
          "chosenTravelMethod": {
            "type": "string"
          },
          "carDurationMinutes": {
            "type": "integer",
            "format": "int32"
          },
          "publicDurationMinutes": {
            "type": "integer",
            "format": "int32"
          },
          "decisionVerdict": {
            "type": "string",
            "nullable": true
          },
          "eurPerMinutePerPerson": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        }
      },
      "CommuteStatisticsDto": {
        "required": [
          "countCarChosen",
          "countPublicChosen",
          "avgDurationCar",
          "avgDurationPublic",
          "avgFuelCostCarChosen",
          "totalSpentEur",
          "totalSavedEur",
          "totalKmCarChosen",
          "totalFuelLitersCarChosen",
          "totalCo2Grams",
          "totalDelaysPublic",
          "goodDecisionsCount",
          "badDecisionsCount"
        ],
        "type": "object",
        "properties": {
          "countCarChosen": {
            "type": "integer",
            "format": "int32"
          },
          "countPublicChosen": {
            "type": "integer",
            "format": "int32"
          },
          "avgDurationCar": {
            "type": "number",
            "format": "double"
          },
          "avgDurationPublic": {
            "type": "number",
            "format": "double"
          },
          "avgFuelCostCarChosen": {
            "type": "number",
            "format": "double",
            "nullable": true
          },
          "totalSpentEur": {
            "type": "number",
            "format": "double"
          },
          "totalSavedEur": {
            "type": "number",
            "format": "double"
          },
          "totalKmCarChosen": {
            "type": "number",
            "format": "double"
          },
          "totalFuelLitersCarChosen": {
            "type": "number",
            "format": "double"
          },
          "totalCo2Grams": {
            "type": "number",
            "format": "double"
          },
          "totalDelaysPublic": {
            "type": "integer",
            "format": "int32"
          },
          "goodDecisionsCount": {
            "type": "integer",
            "format": "int32"
          },
          "badDecisionsCount": {
            "type": "integer",
            "format": "int32"
          }
        }
      },
      "CommuteUploadErrorDto": {
        "required": [
          "errorCode",
          "message"
        ],
        "type": "object",
        "properties": {
          "errorCode": {
            "type": "string"
          },
          "message": {
            "type": "string"
          }
        }
      },
      "PublicCommuteDto": {
        "required": [
          "durationMinutes",
          "delayed"
        ],
        "type": "object",
        "properties": {
          "durationMinutes": {
            "type": "integer",
            "format": "int32"
          },
          "delayed": {
            "type": "boolean"
          }
        }
      }
    }
  },
  "tags": [
    {
      "name": "WebApi"
    },
    {
      "name": "Commutes"
    }
  ]
}